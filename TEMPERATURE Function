import java.util.Scanner;
class HelloWorld {
    public static double celsiusToFahrenheit(double tempConv) {
        return (tempConv * 9 / 5) + 32;
    }
    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        System.out.print("Enter a temperature value (in C): ");
        double tempConv = obj.nextDouble();
        double fahrenheitTemp = celsiusToFahrenheit(tempConv);
        System.out.print("The temperature value in F is: " + fahrenheitTemp);
    }
}
